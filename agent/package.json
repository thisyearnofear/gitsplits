{
  "name": "gitsplits-agent",
  "version": "1.0.0",
  "description": "Autonomous agent for compensating open source contributors",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "dev:cli": "tsx watch src/index.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t gitsplits-agent -f Dockerfile.eigen .",
    "docker:run": "docker run -p 3000:3000 --env-file .env gitsplits-agent"
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.15.10",
    "@octokit/auth-app": "^8.2.0",
    "@octokit/rest": "^20.0.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "near-api-js": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
